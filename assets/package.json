{
	"name": "aquila",
	"version": "1.0.0",
	"description": "Aquila WordPress theme description .. ... .. ... ",
	"main": "main.js",
	"scripts": {
		"prod": "cross-env NODE_ENV=production webpack --mode production --progress",
		"dev": "cross-env NODE_ENV=development webpack --watch --mode development --progress",
		"svg": "svgr -d src/js/icons src/icons",
		"lint": "eslint src/js",
		"lint:fix": "eslint --fix src/js/*",
		"lint-css": "stylelint 'src/sass/**/*.scss'",
		"lint-css:fix": "npm run lint-css --fix",
		"precommit": "npm run lint-css:fix && npm run lint:fix && npm run prod",
		"clean": "rm -rf build/*"
	},
	"keywords": [
		"wordpress",
		"themes"
	],
	"author": "Imran Sayed",
	"license": "MIT",
	"private": true,
	"devDependencies": {
		"@babel/core": "^7.18.9",
		"@babel/preset-env": "^7.11.0",
		"@babel/preset-react": "^7.10.4",
		"@svgr/cli": "^6.3.1",
		"@wordpress/base-styles": "^4.6.0",
		"@wordpress/block-editor": "^9.5.0",
		"@wordpress/blocks": "^11.12.0",
		"@wordpress/components": "^19.15.0",
		"@wordpress/compose": "^5.11.0",
		"@wordpress/data": "^6.13.0",
		"@wordpress/dependency-extraction-webpack-plugin": "^2.8.0",
		"@wordpress/element": "^4.11.0",
		"@wordpress/eslint-plugin": "^12.7.0",
		"@wordpress/hooks": "^3.13.0",
		"@wordpress/i18n": "^4.13.0",
		"@wordpress/icons": "^9.4.0",
		"@wordpress/server-side-render": "^3.11.0",
		"babel-loader": "^8.1.0",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^6.0.3",
		"cross-env": "^7.0.2",
		"css-loader": "^4.2.1",
		"cssnano": "^4.1.10",
		"eslint": "^8.20.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-jsdoc": "^39.3.3",
		"file-loader": "^6.0.0",
		"lodash": "^4.17.21",
		"mini-css-extract-plugin": "^0.9.0",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"sass": "^1.53.0",
		"sass-loader": "^10.3.1",
		"sass-mq": "^6.0.0",
		"style-loader": "^1.2.1",
		"stylelint": "^14.9.1",
		"stylelint-config-wordpress": "^17.0.0",
		"stylelint-webpack-plugin": "^3.3.0",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"url-loader": "^4.1.1",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
	"browserslist": [
		"defaults"
	]
}
